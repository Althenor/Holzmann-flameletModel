FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      H;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions    [ 0 2 -2 0 0 0 0 ];

internalField   uniform -15974.1;

boundaryField
{
    inletfuel           
    {
        type  		fixedValue;
	value		uniform -998143;
    }

    inletair           
    {
        type  		fixedValue;
	value		uniform -15974.1;
    }

    outlet          
    {
        type  		zeroGradient;
    }

    wallheatloose 
    {
        type  		fixedValue;
	value		uniform 0;	// that value is not important
    }

    wallchamber
    {
        type            zeroGradient;
    }
}


