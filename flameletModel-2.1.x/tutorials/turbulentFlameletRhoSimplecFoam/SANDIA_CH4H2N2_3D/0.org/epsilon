FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    inletfuel
    {
	type            compressible::turbulentMixingLengthDissipationRateInlet;
	mixingLength	0.00056;
        value           uniform 596255;
    }
    
    inletair
    {
	type		compressible::turbulentMixingLengthDissipationRateInlet;
	mixingLength	0.02744;
        value           uniform 0.00002;

    }
    
    outlet
    {
        type            zeroGradient;
    }
    
    axis
    {
        type            empty;
    }
    
    "wall.*"
    {
        type 		compressible::epsilonWallFunction;
	value		uniform 0.00041;
    }
    
    front
    {
        type            wedge;
    }
    
    back
    {
        type            wedge;
    }
}


